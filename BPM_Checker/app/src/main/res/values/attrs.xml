<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BarGraph">
        <attr name="bar_color" format="color|reference"/>
        <attr name="max" format="float"/>
        <attr name="item_size" format="integer"/>
        <attr name="interval" format="float"/>
        <attr name="bar_text_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="Circle">
        <attr name="circle_color" format="color|reference"/>
        <attr name="stroke_width" format="float|reference"/>
        <attr name="circle_max" format="float|reference"/>
        <attr name="circle_text_color" format="color|reference"/>
    </declare-styleable>

</resources>